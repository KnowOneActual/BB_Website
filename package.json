{
    "name": "beaubremer-weather-bot",
    "version": "1.0.0",
    "scripts": {
        "test": "jest",
        "test:watch": "jest --watch",
        "test:coverage": "jest --coverage",
        "build:css": "tailwindcss -i ./src/input.css -o ./style.css --minify",
        "build:css:silent": "BROWSERSLIST_IGNORE_OLD_DATA=1 tailwindcss -i ./src/input.css -o ./style.css --minify",
        "update-browsers": "npx browserslist@latest --update-db",
        "postinstall": "npx update-browserslist-db@latest"
    },
    "dependencies": {
        "node-fetch": "^2.6.7",
        "rss-parser": "^3.13.0"
    },
    "devDependencies": {
        "@testing-library/jest-dom": "^6.1.5",
        "jest": "^29.7.0",
        "jest-environment-jsdom": "^29.7.0",
        "tailwindcss": "^3.4.1"
    }
}
